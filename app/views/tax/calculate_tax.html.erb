<script>
function myFunction() {
  var salary = document.getElementById("salary").value;
  document.getElementById("monthly-salary").innerHTML = salary;
  if (salary == 0) {document.getElementById("monthly-salary").innerHTML = 0}
  document.getElementById("monthly-tax").innerHTML=calculate(salary);
  document.getElementById("salary-after-tax").innerHTML=salary-document.getElementById("monthly-tax").innerHTML;
  document.getElementById("yearly-salary").innerHTML=salary*12;
  document.getElementById("yearly-tax").innerHTML=document.getElementById("monthly-tax").innerHTML*12;
  document.getElementById("yearly-income-after-tax").innerHTML=document.getElementById("yearly-salary").innerHTML-document.getElementById("yearly-tax").innerHTML;
}

function calculate(value) {
  var tax;
  if (value < 50000) {
    tax = 0;
  }
  else if (50000 < value && value <= 100000) {
    tax = (value - 50000) * 0.000166;
  }
  else if (100000 < value && value <= 200000) {
    tax = (value - 100000) * 0.07;
  }
  else if (200000 < value && value <= 300000) {
    tax = ((value - 200000) * 0.125) + 7000;
  }
  else if (300000 < value && value <= 500000) {
    tax = ((value - 300000) * 0.175) + 19500;
  }
  else if (200000 < value && value <= 1000000) {
    tax = ((value - 500000) * 0.225) + 54500;
  }
  else if (value <= 1000000) {
    tax = ((value - 100000) * 0.325) + 170000;
  }
  return Math.round(tax);
}

</script>
